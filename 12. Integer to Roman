class Solution {
public:
    string intToRoman(int num) {
        vector<string> s= {"M", "CM", "D", "CD", "C", "XC", "L", "XL", "X", "IX", "V", "IV", "I"};
        vector<int> n= {1000, 900, 500, 400, 100, 90, 50, 40, 10, 9, 5,4,1};
        int i=0;
        string ans="";
        while(num!=0 && i<s.size()){
            if(num>=n[i]){
                ans+=s[i];
                num-=n[i];
            } 
            else i++;
            
        }
        return ans;
    }
};
